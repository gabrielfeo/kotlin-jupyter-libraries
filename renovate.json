{
  "enabled": true,
  "extends": [
    "group:recommended",
    "workarounds:all"
  ],
  "rebaseWhen": "conflicted",
  "regexManagers": [
    {
      "datasourceTemplate": "maven",
      "fileMatch": [".*\\.json"],
      "matchStringsStrategy": "any",
      "matchStrings": [
        "\"(?<depName>[^\":]+:[^\":]+):(?<currentValue>[^\":]+)\""
      ]
    }
  ]
}
